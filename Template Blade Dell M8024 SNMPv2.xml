            <discovery_rules>
                <discovery_rule>
                    <name>FAN Discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10895.3000.1.2.110.7.1.1.2]</snmp_oid>
                    <key>envMonFanStatusDescr</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>$1 state</name>
                            <type>SNMP_AGENT</type>
